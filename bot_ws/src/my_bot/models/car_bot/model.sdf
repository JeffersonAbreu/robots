<?xml version="1.0"?>
<sdf version="1.8">
  <model name='car_bot'>
    <link name='chassis_link'>
      <pose relative_to='__model__'>0 0 0.05 0 0 3.14159265359</pose>
      <inertial>
        <mass>0.12859</mass>
        <inertia>
          <ixx>5.2146E-05</ixx>
          <ixy>-1.2223E-19</ixy>
          <ixz>2.9403E-20</ixz>
          <iyy>0.00017957</iyy>
          <iyz>1.8099E-14</iyz>
          <izz>0.00022799</izz>
        </inertia>
      </inertial>
      <visual name='visual'>
        <geometry>
          <mesh>
            <uri>model://meshes/chassis_link.stl</uri>
            <scale>1 1 1</scale>
          </mesh>
        </geometry>
        <material>
          <ambient>0.5 0.75 0.5 1</ambient>
          <diffuse>0.7 0.9 0 1</diffuse>
          <specular>0.2 0.2 0.2 1</specular>
        </material>
      </visual>
      <collision name='collision'>
        <geometry>
          <mesh>
            <uri>model://meshes/chassis_link.stl</uri>
            <scale>1 1 1</scale>
          </mesh>
        </geometry>
      </collision>
    </link>

    <!-- Roda Esquerda -->
    <link name='left_wheel'>
      <pose relative_to="chassis_link">-0.033259 0.08025 -0.01375 -1.570796327 0.042481 0</pose>
      <inertial>
        <mass>0.0021404</mass>
        <inertia>
          <ixx>8.6283E-07</ixx>
          <iyy>8.6269E-07</iyy>
          <izz>1.4515E-06</izz>
        </inertia>
      </inertial>

      <visual name='visual'>
        <geometry>
          <mesh>
            <uri>model://meshes/roda_esquerda_link.stl</uri>
            <scale>1 1 1</scale>
          </mesh>
        </geometry>
        <material>
          <ambient>1 0 0 1</ambient>
          <diffuse>1 0 0 1</diffuse>
          <specular>1 0 0 1</specular>
        </material>
      </visual>
      <collision name='collision'>
        <geometry>
          <mesh>
            <uri>model://meshes/roda_esquerda_link.stl</uri>
            <scale>1 1 1</scale>
          </mesh>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu>1.0</mu>
              <mu2>1.0</mu2>
            </ode>
          </friction>
        </surface>
      </collision>
    </link>

    <link name='right_wheel'>
      <pose relative_to="chassis_link">-0.033259 -0.08025 -0.01375 1.570796327 0.042481
        -3.14159265359</pose>
      <inertial>
        <mass>0.0021404</mass>
        <inertia>
          <ixx>8.6283E-07</ixx>
          <iyy>8.6269E-07</iyy>
          <izz>1.4515E-06</izz>
        </inertia>
      </inertial>
      <visual name='visual'>
        <geometry>
          <mesh>
            <uri>model://meshes/roda_direita_link.stl</uri>
            <scale>1 1 1</scale>
          </mesh>
        </geometry>
        <material>
          <ambient>1 0 0 1</ambient>
          <diffuse>1 0 0 1</diffuse>
          <specular>1 0 0 1</specular>
        </material>
      </visual>
      <collision name='collision'>
        <geometry>
          <mesh>
            <uri>model://meshes/roda_direita_link.stl</uri>
            <scale>1 1 1</scale>
          </mesh>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu>1.0</mu>
              <mu2>1.0</mu2>
            </ode>
          </friction>
        </surface>
      </collision>
    </link>

    <frame name="caster_frame" attached_to='chassis_link'>
      <pose>0.072581 0 -0.0025 3.14159 0 0</pose>
    </frame>

    <!-- Roda Castor -->
    <link name='base_castor_link'>
      <pose relative_to='caster_frame'>0 0 0 0 0 0</pose>
      <inertial>
        <mass>0.0001</mass>
        <inertia>
          <ixx>1.6667E-07</ixx>
          <iyy>1.6667E-07</iyy>
          <izz>1.6667E-07</izz>
        </inertia>
      </inertial>
      <visual name='visual'>
        <geometry>
          <mesh>
            <uri>model://meshes/base_castor_link.stl</uri>
            <scale>1 1 1</scale>
          </mesh>
        </geometry>
        <material>
          <ambient>0.0 1 0.0 1</ambient>
          <diffuse>0.0 1 0.0 1</diffuse>
          <specular>0.0 1 0.0 1</specular>
        </material>
      </visual>
      <collision name='collision'>
        <geometry>
          <mesh>
            <uri>model://meshes/base_castor_link.stl</uri>
            <scale>1 1 1</scale>
          </mesh>
        </geometry>
      </collision>
    </link>

    <link name='castor_wheel_link'>
      <pose relative_to="base_castor_link">0.0152 0 0.0231 1.570796327 0 0</pose>
      <inertial>
        <mass>0.00828739992319076</mass>
        <inertia>
          <ixx>6.3926680927577E-07</ixx>
          <ixy>-7.31657239076889E-23</ixy>
          <ixz>4.99800544060975E-23</ixz>
          <iyy>6.39299912774584E-07</iyy>
          <iyz>3.50834634007177E-09</iyz>
          <izz>1.01108533248037E-06</izz>
        </inertia>
      </inertial>
      <visual name='visual'>
        <geometry>
          <mesh>
            <uri>model://meshes/roda_castor_link.stl</uri>
            <scale>1 1 1</scale>
          </mesh>
        </geometry>
        <material>
          <ambient>1 0 0 1</ambient>
          <diffuse>1 0 0 1</diffuse>
          <specular>1 0 0 1</specular>
        </material>
      </visual>
      <collision name='collision'>
        <geometry>
          <mesh>
            <uri>model://meshes/roda_castor_link.stl</uri>
            <scale>1 1 1</scale>
          </mesh>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu>1.0</mu>
              <mu2>1.0</mu2>
            </ode>
          </friction>
        </surface>
      </collision>
    </link>

    <model name='lidar_link'>
      <self_collide>true</self_collide>
      <pose relative_to='chassis_link'>-0.07 0 0.007 0 0 0</pose>
      <include merge="true">
        <uri>model://lidar</uri>
      </include>
    </model>


    <model name="imu">
      <self_collide>true</self_collide>
      <pose relative_to='chassis_link'>0 0 0 0 0 0</pose>
      <include merge="true">
        <uri>model://imu</uri>
      </include>
    </model>

    <joint name='lidar_joint' type='fixed'>
      <parent>chassis_link</parent>
      <child>lidar_link</child>
    </joint>

    <joint name='imu_joint' type='fixed'>
      <parent>chassis_link</parent>
      <child>imu</child>
    </joint>

    <!-- Junta da Roda Esquerda -->
    <joint name='left_wheel_joint' type='revolute'>
      <pose relative_to='left_wheel' />
      <parent>chassis_link</parent>
      <child>left_wheel</child>
      <axis>
        <xyz expressed_in='__model__'>0 1 0</xyz>
        <limit>
          <lower>-1.79769e+308</lower>
          <upper>1.79769e+308</upper>
        </limit>
      </axis>
    </joint>

    <!-- Junta da Roda Direita -->
    <joint name='right_wheel_joint' type='revolute'>
      <pose relative_to='right_wheel' />
      <parent>chassis_link</parent>
      <child>right_wheel</child>
      <axis>
        <xyz expressed_in='__model__'>0 1 0</xyz>
        <limit>
          <lower>-1.79769e+308</lower>
          <upper>1.79769e+308</upper>
        </limit>
      </axis>
    </joint>

    <joint name='base_castor_joint' type='revolute'>
      <pose relative_to='base_castor_link' />
      <parent>chassis_link</parent>
      <child>base_castor_link</child>
      <axis>
        <xyz expressed_in='__model__'>0 0 1</xyz>
        <limit>
          <lower>-1.79769e+308</lower>
          <upper>1.79769e+308</upper>
        </limit>
      </axis>
    </joint>

    <joint name='castor_wheel_joint' type='revolute'>
      <pose relative_to='castor_wheel_link' />
      <parent>base_castor_link</parent>
      <child>castor_wheel_link</child>
      <axis>
        <xyz expressed_in='__model__'>0 1 0</xyz>
        <limit>
          <lower>-1.79769e+308</lower>
          <upper>1.79769e+308</upper>
        </limit>
      </axis>
    </joint>

    <plugin filename="gz-sim-diff-drive-system" name="gz::sim::systems::DiffDrive">
      <left_joint>left_wheel_joint</left_joint>
      <right_joint>right_wheel_joint</right_joint>
      <wheel_separation>0.16050</wheel_separation>
      <wheel_radius>0.034</wheel_radius>
      <topic>/cmd_vel</topic>
      <odom_publish_frequency>100</odom_publish_frequency>
      <odom_topic>/odom</odom_topic>
    </plugin>
  </model>
</sdf>